%h1= "Category \"#{@category.to_param}\""
- content_tag_for :div, @category do
  %ul
    %li
      %strong= _('Nazwa:')
      =h @category.name
    %li
      %strong= _('Opis:')
      =h @category.body

= link_to _('Edit'), edit_admin_category_path(@category)
= link_to _('Back'), admin_categories_path
